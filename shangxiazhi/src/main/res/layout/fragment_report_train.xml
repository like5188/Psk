            android:textSize="14sp"
            android:textStyle="bold"
            app:layout_constraintBaseline_toBaselineOf="@id/tvDurationTag"
            app:layout_constraintStart_toEndOf="@id/tvActiveDurationTag"
            tools:text="00秒" />

        <TextView
            android:id="@+id/tvPassiveDurationTag"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:text="被动"
            android:textColor="#686868"
            android:textSize="14sp"
            app:layout_constraintBaseline_toBaselineOf="@id/tvDurationTag"
